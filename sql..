


#weather observation station 19
 SELECT ROUND(SQRT(POW(MAX(LAT_N)-MIN(LAT_N),2)+POW(MAX(LONG_W)-MIN(LONG_W),2)),4) FROM STATION;


#The Pads
SELECT CONCAT(NAME,'(',LEFT(OCCUPATION, 1),')') 
FROM OCCUPATIONS 
ORDER BY NAME;
select CONCAT('There are a total of ',count(*),' ',lower(occupation), 's.') from occupations
group by occupation order by count(*),occupation;
